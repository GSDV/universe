{
  "expo": {
    "name": "UniVerse Campus",
    "slug": "UniVerse",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gsdv-app-joinuniverse",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "infoPlist": {
        "displayName": "UniVerse",
        "CFBundleDisplayName": "UniVerse",
        "NSCameraUsageDescription": "Allow access to your camera to take photos and videos for your posts.",
        "NSPhotoLibraryUsageDescription": "Allow access to your photo library to share photos and videos in your posts.",
        "NSLocationWhenInUseUsageDescription": "Turning on location allows you to post on the map and view other map posts.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "supportsTablet": false,
      "bundleIdentifier": "me.gsdv.UniVerse",
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "me.gsdv.UniVerse"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "$(PRODUCT_NAME) needs your location to interact with the map."
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "newArchEnabled": false,
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b8fbf108-0761-46eb-b6fb-ef8bf609f083"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b8fbf108-0761-46eb-b6fb-ef8bf609f083"
    }
  }
}
