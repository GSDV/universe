{
  "cli": {
    "version": ">= 14.4.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": false,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "simulator": false
      },
      "channel": "preview",
      "env": {
        "EXPO_PUBLIC_DOMAIN": "https://joinuniverse.app"
      }
    },

    "production": {
        "distribution": "store",
        "ios": {
          "resourceClass": "m-medium",
          "simulator": false
        },
        "channel": "production",
        "autoIncrement": true,
        "env": {
          "EXPO_PUBLIC_DOMAIN": "https://joinuniverse.app"
        }
      }
  },
  "submit": {
    "production": {}
  }
}
